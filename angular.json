{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "site": {
      "root": "src/main/webapp/site",
      "sourceRoot": "src/main/webapp/site/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "src/main/webapp/site/dist",
            "index": "src/main/webapp/site/src/index.html",
            "main": "src/main/webapp/site/src/main.ts",
            "tsConfig": "src/main/webapp/site/src/tsconfig.app.json",
            "polyfills": "src/main/webapp/site/src/polyfills.ts",
            "extractCss": true,
            "assets": [
              "src/main/webapp/site/src/assets",
              "src/main/webapp/site/src/favicon.ico"
            ],
            "styles": [
              "src/main/webapp/site/src/style/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "statsJson": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/main/webapp/site/src/environments/environment.ts",
                  "with": "src/main/webapp/site/src/environments/environment.prod.ts"
                }
              ],
              "outputPath": "src/main/webapp/site/dist/",
              "baseHref": "/"
            },
            "production-es": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "statsJson": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/main/webapp/site/src/environments/environment.ts",
                  "with": "src/main/webapp/site/src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/main/webapp/site/src/locale/messages.xlf",
                  "with": "src/main/webapp/site/src/locale/messages.es.xlf"
                }
              ],
              "outputPath": "src/main/webapp/site/dist/es/",
              "baseHref": "/es/",
              "i18nFile": "src/main/webapp/site/src/locale/messages.es.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "es"
            },
            "production-zh-Hans": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "statsJson": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/main/webapp/site/src/environments/environment.ts",
                  "with": "src/main/webapp/site/src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/main/webapp/site/src/locale/messages.xlf",
                  "with": "src/main/webapp/site/src/locale/messages.zh-Hans.xlf"
                }
              ],
              "outputPath": "src/main/webapp/site/dist/zh-Hans/",
              "baseHref": "/zh-Hans/",
              "i18nFile": "src/main/webapp/site/src/locale/messages.zh-Hans.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "zh-Hans"
            },
            "production-zh-Hant": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "statsJson": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/main/webapp/site/src/environments/environment.ts",
                  "with": "src/main/webapp/site/src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/main/webapp/site/src/locale/messages.xlf",
                  "with": "src/main/webapp/site/src/locale/messages.zh-Hant.xlf"
                }
              ],
              "outputPath": "src/main/webapp/site/dist/zh-Hant/",
              "baseHref": "/zh-Hant/",
              "i18nFile": "src/main/webapp/site/src/locale/messages.zh-Hant.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "zh-Hant"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "site:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "site:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "site:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/main/webapp/site/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/main/webapp/site/src/polyfills.ts",
            "tsConfig": "src/main/webapp/site/src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/main/webapp/site/src/style/styles.scss"
            ],
            "assets": [
              "src/main/webapp/site/src/assets",
              "src/main/webapp/site/src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "site-e2e": {
      "root": "src/main/webapp/site/e2e",
      "sourceRoot": "src/main/webapp/site/e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "site:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
